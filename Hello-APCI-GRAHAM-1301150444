Program Hello; // ubah menjadi Program APCI-NIM
            Uses Crt;
            Begin
                ClrScr;
                Writeln(‘Hello’);//untuk mengeluarkan string ‘Hello’ di layar
                Writeln(‘1301150444’); //Ubah untuk menampilkan NIM Anda
                Writeln(‘Fadila Zain’); //Ubah untuk menampilkan Nama Anda
                Writeln(‘if 39-10’); //Ubah untuk menampilkan Kelas Anda
                Writeln(‘GRAHAM’); //Ubah untuk menampilkan Nama Asprak Anda
                Writeln(‘1301154486’); //Ubah untuk menampilkan NIM teman anda
                Writeln(‘Modul 6’); //Ubah untuk menampilkan string ‘Modul 6
            End.